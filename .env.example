# Car Log Environment Variables
# Copy this file to .env and fill in your values

# Required: OpenAI API key for DSPy modules
OPENAI_API_KEY=sk-...

# Optional: Anthropic API key for dashboard OCR (Claude Vision)
ANTHROPIC_API_KEY=sk-ant-...

# MLflow tracking mode: "full" (detailed tracing) or "summary" (aggregate metrics only)
MLFLOW_TRACKING_MODE=summary
